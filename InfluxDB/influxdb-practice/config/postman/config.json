							"            <th>Measurement</th>",
							"            <th>Time</th>",
							"            <th>Value</th>",
							"        </tr>",
							"",
							"        {{#each response.user}}",
							"           <tr bgcolor=\"white\">",
							"                <td>user</td>",
							"                <td>{{recordAt}}</td>",
							"                <td class=\"value\">{{value}}</td>",
							"            </tr>",
							"        {{/each}}",
							"",
							"        {{#each response.system}}",
							"           <tr bgcolor=\"white\">",
							"                <td>system</td>",
							"                <td>{{recordAt}}</td>",
							"                <td class=\"value\">{{value}}</td>",
							"            </tr>",
							"        {{/each}}",
