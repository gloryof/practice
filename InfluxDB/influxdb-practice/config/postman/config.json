							"                <td>idle</td>",
							"                <td>{{recordAt}}</td>",
							"                <td class=\"value\">{{value}}</td>",
							"            </tr>",
							"        {{/each}}",
							"",
							"        {{#each response.per1Minutes}}",
							"           <tr bgcolor=\"white\">",
							"                <td>per1Minutes</td>",
							"                <td>{{recordAt}}</td>",
							"                <td class=\"value\">{{value}}</td>",
							"            </tr>",
							"        {{/each}}",
							"",
							"        {{#each response.per5Minutes}}",
