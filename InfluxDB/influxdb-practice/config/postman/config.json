							"                <td>{{recordAt}}</td>",
							"                <td class=\"value\">{{value}}</td>",
							"            </tr>",
							"        {{/each}}",
							"",
							"        {{#each response.per15Minutes}}",
							"           <tr bgcolor=\"white\">",
							"                <td>per15Minutes</td>",
							"                <td>{{recordAt}}</td>",
							"                <td class=\"value\">{{value}}</td>",
							"            </tr>",
							"        {{/each}}",
